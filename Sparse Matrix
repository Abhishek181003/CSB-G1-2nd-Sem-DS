#include <stdio.h>
int isSparse(int a[3][3] , int row , int column ){
    float count=0;
    for(int i =0 ; i<row; i++){ 
        for(int j=0; j<column; j++){
            if(a[i][j] != 0) count++; 
        }
    }
    printf("%f",count);
    if(count/(float)(row*column) <= 1.0/3.0) {
        printf("Yes this sparse");
        return 1;
    }
    else return 0;
}
int main() {
    printf("Enter the Row and Column of the Sparse Matrix : ");
        int count=0;
        int row,column;
        scanf("%d%d",&row,&column);
        int arr[row][column];
        printf("Enter %d elements \n",row*column);
        for(int i =0 ; i<row; i++){
            for(int j=0; j<column; j++)
                scanf("%d",&arr[i][j]);
        }
        printf("The Matrix you entered\n");
        for(int i =0 ; i<row; i++){
            for(int j=0; j<column; j++){
                printf("%d ",arr[i][j]);
            }
            printf("\n");
        }
    if(isSparse(arr,row,column)){
        printf("sparse");
    }else{
        printf("The given matrix is not Sparse Matrix ");
        return 0;
    }
    
    return 0;
}
